# Configurações do Banco de Dados
DATABASE_URL=postgresql://username:password@localhost:5432/clashroyaledb

# Chave API do Clash Royale
# Obtenha em: https://developer.clashroyale.com
CLASH_ROYALE_API_KEY=sua_chave_api_aqui

# Segredo para sessões (gerar um valor seguro)
SESSION_SECRET=seu_segredo_super_seguro_aqui

# Porta do servidor (opcional)
PORT=5000

# Ambiente (development/production)
NODE_ENV=development
